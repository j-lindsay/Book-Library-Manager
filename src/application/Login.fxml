<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <PasswordField fx:id="passwordTxt" layoutX="621.0" layoutY="272.0" prefHeight="37.0" prefWidth="532.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <TextField fx:id="usernameTxt" layoutX="621.0" layoutY="207.0" prefHeight="37.0" prefWidth="532.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="loginButton" layoutX="621.0" layoutY="334.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="loginStatus" layoutX="621.0" layoutY="385.0" prefHeight="39.0" prefWidth="532.0" text="Status">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox fx:id="leftHBoxAesth" prefHeight="769.0" prefWidth="592.0" />
      <Button fx:id="registerButton" layoutX="702.0" layoutY="334.0" mnemonicParsing="false" onAction="#changeToRegistration" text="Register">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
